{
  "name": "Luna_bot",
  "logo": "https://telegra.ph/file/de65abf86cfde85772e21.jpg",
  "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat.and get information about movies and tv series any many more .",
  "keywords": ["music", "voicechat", "subtitles", "movie", "series", "telegram"],
  "repository": "https://github.com/Navipavi6818/luna_bot",
  "stack": "container",
  "buildpacks": [
   {
     "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
   },
   {
     "url": "heroku/python"
   }   
  ],        
  "env": {    
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather",
      "required": true
    },
    "BOT_USERNAME": {
      "description": "Your bot's username without @",
      "required": true
    },
    "BOT_NAME": {
      "description": "Your MusicPlayer Bot Name.",
      "required": false,
      "value": ""
    },
    "Name": {
      "description": "Your telegram name [Leave this if you don't have one].",
      "required": false,
      "value": "MrProfessor"
    },
    "USERNAME": {
      "description": "Your telegram USERNAME without @ [Leave this if you don't have one].",
      "required": false,
      "value": "N_A_V_I_P_A_V_I"
    },
    "SUPPORT_GROUP": {
      "description": "Your MusicPlayer support group without @ [Leave this if you don't have one]",
      "required": false,
      "value": "DaisySupport_Official"
    },
    "PROJECT_NAME": {
      "description": "Your MusicPlayer project Name.",
      "required": false,
      "value": "DaisyXMusic v5"
    },
    "ASSISTANT_NAME": {
      "description": "Your MusicPlayer's assistant Username without @.",
      "required": true
    },
    "BG_IMAGE": {
      "description": "BG image of bot (Use transparent one) [IGNORE].",
      "required": false,
      "value": " https://telegra.ph/file/dcfdf612e499eef0e0b1f.png"
    },     
    "UPDATES_CHANNEL": {
      "description": "Updates Channel without @ [Leave this if you don't have one].",
      "required": false,
      "value": "DaisyXupdates"
    },    
    "API_ID": {
      "description": "App ID from my.telegram.org/apps",
      "required": true
    },    
    "API_HASH": {
      "description": "App hash from my.telegram.org/apps",
      "required": true      
    },
    "TMDBAPI": {
      "description": "your TMDBAPI.",
      "required": true
    }
  }
}
